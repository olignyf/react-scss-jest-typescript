{
  "name": "react-scss-jest-typescript",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/path-to-regexp": "^1.7.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "bootstrap": "4.6.0",
    "formik": "^2.4.2",
    "i18next-browser-languagedetector": "^6.1.0",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "natsort": "^2.0.3",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "sass-loader": "14.1.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yarn": "^1.22.19",
    "yup": "^1.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts",
    "jest": "jest --env=node --rootDir='.' --modulePaths='.' --roots='src/__tests__' "
  },
  "jest": {
    "testMatch": [
      "**/?(*.)+(spec).[jt]s?(x)"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs|tsx|ts)$": "babel-jest",
      "^.+\\.svg$": "jest-transform-file",
      "^.+\\.css$": "jest-transform-css",
      "^.+\\.less$": "jest-transform-css",
      "^.+\\.scss$": "jest-transform-scss"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(bootstrap-less-port|@somepackage))"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "babel-jest": "^26.6.3",
    "eslint": "^8.44.0",
    "i18next": "^20.1.0",
    "jest": "^26.6.3",
    "jest-image-snapshot": "^4.4.1",
    "jest-transform-css": "^2.1.0",
    "jest-transform-file": "^1.1.1",
    "jest-transform-scss": "^1.0.8",
    "jest-watch-typeahead": "0.6.5",
    "react-i18next": "^11.8.12",
    "sass-jest": "^0.1.7",
    "visual-screenshot": "^1.0.1"
  },
  "resolutions": {
    "node-sass": "^6.0.1",
    "sass-loader": "10.0.5",
    "react-error-overlay": "6.0.9"
  }
}
